    "intl": "^1.2.5",
    "react-langapi": "^1.0.11"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "babel-plugin-transform-remove-strict-mode": "^0.0.2",
    "babel-preset-gatsby-package": "^0.2.2",
    "langapi-client": "^1.4.10",
    "prettier": "^1.18.2",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "rimraf": "^2.6.3",
    "webpack": "^4.35.3"
  },
  "homepage": "https://github.com/cyrieu/gatsby-plugin-lang",
  "keywords": [
