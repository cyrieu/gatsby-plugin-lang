{
  "name": "gatsby-plugin-lang",
  "description": "Gatsby plugin to add Lang onto a site",
  "version": "1.0.0",
  "author": "Eric Yu <cyrieu@gmail.com>",
  "bugs": {
    "url": "https://github.com/cyrieu/gatsby-plugin-lang"
  },
  "dependencies": {
