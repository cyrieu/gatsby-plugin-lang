    "intl"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^2.0.0",
    "langapi-client": "^1.4.10",
    "react": "^16.8.2",
    "webpack": "^4.35.3"
  },
  "repository": "https://github.com/cyrieu/gatsby-plugin-lang",
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "NODE_ENV=production yarn build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__",
